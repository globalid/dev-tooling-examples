{
  "name": "verifier-agent-example",
  "version": "0.1.0",
  "description": "An example implementation of a verifier agent using NestJS",
  "author": "GlobaliD",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run lint && npm run format:check",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=1"
  },
  "dependencies": {
    "@globalid/verifier-toolkit": "^0.2.0",
    "@nestjs/common": "8.4.4",
    "@nestjs/config": "1.2.0",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/platform-socket.io": "^8.4.7",
    "@nestjs/websockets": "8.4.4",
    "axios": "0.26.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dayjs": "^1.11.3",
    "hbs": "4.2.0",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "node-cache": "^5.1.2",
    "reflect-metadata": "0.1.13",
    "ws": "8.5.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.2",
    "@jest/types": "^28.1.0",
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/testing": "8.4.4",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/supertest": "2.0.12",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "socket.io-client": "^4.5.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.2",
    "ts-jest": "28.0.0-next.3",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3",
    "webpack": "5.72.0"
  }
}
